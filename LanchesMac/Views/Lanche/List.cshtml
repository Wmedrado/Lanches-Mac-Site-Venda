@model IEnumerable<LanchesMac.Models.Lanche>

<div>
    <h2>Todos os Lanches</h2>
</div>

@foreach (var lanche in Model)
{
    <div>
        <h4>@lanche.Nome</h4>
        <h1>teste</h1>
        <p><img src="@lanche.ImagemThumbnailUrl"></p>
        <h3>Preço :R @lanche.Preco.ToString("c")</h3>
    </div>
}